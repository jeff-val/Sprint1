pragma solidity 0.8.18;
contract SmartContract { 
    
    function sumade2numeros(uint x, uint y) public pure returns (uint){
        return x+y;
    }
    function productode2numeros(uint x, uint y) public pure returns (uint){
        return x*y;
    }
    function productodefunciones(uint x, uint y) external pure returns (uint){
        return sumade2numeros(x,y) * productode2numeros(x,y);
    }
}
